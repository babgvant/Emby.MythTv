      <xs:element minOccurs="0" name="DiscLang" type="xs:string" />
      <xs:element minOccurs="0" name="DiscNum" type="xs:unsignedInt" />
      <xs:element minOccurs="0" name="TotalDiscNum" type="xs:unsignedInt" />
